<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<HBox
        xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        fx:controller="mygame.editor.controlers.ToolbarController"

        alignment="CENTER_LEFT" spacing="10.0">
    <children>


        <HBox alignment="CENTER"
              minWidth="-Infinity"
              minHeight="-Infinity"
              fx:id="mainActions"
              styleClass="container_border">

            <padding>
                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
            </padding>

        </HBox>

        <HBox fx:id="secondaryActions" alignment="CENTER" minWidth="-Infinity" minHeight="-Infinity"
              styleClass="container_border">

            <children>

            </children>
            <padding>
                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
            </padding>
        </HBox>

    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>

</HBox>